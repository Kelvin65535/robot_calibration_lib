# - Config file for the calibration package
# It defines the following variables
#  CALIBRATION_INCLUDE_DIR - include directory
#  CALIBRATION_LIBRARIES    - libraries to link against

# Compute paths
get_filename_component(CALIBRATION_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(CALIBRATION_INCLUDE_DIR "@CONFIG_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
include("${CALIBRATION_CMAKE_DIR}/calibration-targets.cmake")

# These are IMPORTED targets created by calibration-targets.cmake
set(CALIBRATION_LIBRARIES "@CALIBRATION_EXPORT_TARGETS@")
